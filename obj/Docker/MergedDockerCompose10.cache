name: dockercompose9250730866613271009
services:
  pgadmin:
    container_name: pgadmin4_container
    environment:
      PGADMIN_DEFAULT_EMAIL: admin@admin.com
      PGADMIN_DEFAULT_PASSWORD: root
    image: dpage/pgadmin4
    networks:
      dockerapi-dev: null
    ports:
    - mode: ingress
      target: 80
      published: "8050"
      protocol: tcp
    restart: unless-stopped
  postgres_image:
    environment:
      POSTGRES_DB: blogdb
      POSTGRES_PASSWORD: bloguser
      POSTGRES_USER: bloguser
    image: postgres:latest
    networks:
      dockerapi-dev: null
    ports:
    - mode: ingress
      target: 5432
      published: "5433"
      protocol: tcp
    restart: unless-stopped
    volumes:
    - type: volume
      source: db_volume
      target: /var/lib/postgresql/data
      volume: {}
  webapi:
    build:
      context: C:\Proyectos\Deuz\SolutionDeuz
      dockerfile: WebApi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebApi
      target: base
    container_name: WebApi_1
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DB_CONNECTION_STRING: host=postgres_image;port=5432;database=blogdb;username=bloguser;password=bloguser
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: webapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/WebApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      dockerapi-dev: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 80
      published: "8000"
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Proyectos\Deuz\SolutionDeuz\WebApi
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Proyectos\Deuz\SolutionDeuz
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\jpino\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\jpino\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\jpino\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\jpino\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  dockerapi-dev:
    name: dockercompose9250730866613271009_dockerapi-dev
    driver: bridge
volumes:
  db_volume:
    name: dockercompose9250730866613271009_db_volume